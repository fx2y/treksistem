{
  "name": "@treksistem/eslint-config-custom",
  "version": "0.0.0",
  "private": true,
  "description": "Shared ESLint configuration for Treksistem monorepo",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./next": "./next.js",
    "./svelte": "./svelte.js"
  },
  "files": [
    "index.js",
    "next.js",
    "svelte.js"
  ],
  "scripts": {
    "format": "prettier --write \"*.{js,ts,tsx,json,md}\" --cache"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": false
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.46.1",
    "svelte-eslint-parser": "^0.41.0"
  }
}
